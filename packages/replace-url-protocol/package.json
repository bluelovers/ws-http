{
  "name": "replace-url-protocol",
  "version": "1.0.4",
  "description": "helper for avoid node.js can't update protocol for some url",
  "homepage": "https://github.com/bluelovers/ws-http/tree/master/packages/replace-url-protocol#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-http/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-http.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "scripts": {
    "coverage": "yarn run test -- --coverage",
    "lint": "ynpx --quiet eslint -- **/*.ts",
    "test": "jest --passWithNoTests",
    "test:jest": "jest --passWithNoTests",
    "test:mocha": "ynpx --quiet -p ts-node -p mocha mocha -- --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "preversion": "echo preversion && yarn run test",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "ncu": "yarn-tool ncu -u",
    "sort-package-json": "yarn-tool sort"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  }
}
